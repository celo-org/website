    "front-matter": "^4.0.1",
    "fuzzy-search": "^3.2.1",
    "graphql": "^16.0.1",
    "graphql-request": "^3.6.1",
    "helmet": "^4.6.0",
    "hex-rgba": "^1.0.2",
    "html-to-formatted-text": "^2.7.0",
    "js-cookie": "^3.0.1",
    "lodash.throttle": "^4.1.1",
    "lottie-web": "^5.8.1",
    "markdown-to-jsx": "^7.1.3",
    "mobile-detect": "^1.4.4",
    "net": "^1.0.2",
    "network-speed": "^2.1.1",
    "next": "^12.0.7",
    "next-i18next": "^8.8.0",
