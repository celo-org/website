    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^7.22.1",
    "react-is": "^17.0.1",
    "react-lazyload": "^3.2.0",
    "react-lazyload-fadein": "^1.1.0",
    "react-modal": "^3.14.3",
    "react-native-web": "^0.17.5",
    "react-youtube": "^7.13.1",
    "scroll-into-view": "^1.16.0",
    "shuffle-seed": "^1.1.6",
    "stickyfilljs": "^2.1.0",
    "swr": "^1.0.1",
    "yiq": "^4.0.0"
