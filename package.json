    "lint-staged": "^11.1.2",
    "next-sitemap": "^1.6.57",
    "nodemon": "^2.0.2",
    "prettier": "^2.3.0",
    "react-native-svg": "^9.13.6",
    "react-test-renderer": "^17.0.2",
