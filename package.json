    "@types/react-native": "0.63.18",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-plugin-module-resolver": "^4.0.0",
